{
  "responsive-layout.desktop#menu-institucional-desktop": {
    "title": "Desktop",
    "children": [
      "flex-layout.col#menu-institucional-desktop"
    ]
  },

  "flex-layout.col#menu-institucional-desktop": {
    "title": "Container Menu",
    "props": {
      "preventVerticalStretch": true,
      "blockClass": "container__menu-institucional-desktop-flex-col"
    },
    "children": [
      "vtex.menu@2.x:menu#menu-institucional-desktop"
    ]
  },

  "vtex.menu@2.x:menu#menu-institucional-desktop": {
    "props": {
      "orientation": "vertical",
      "blockClass": "container__menu-institucional-desktop-menu"
    },
    "children": [
      "rich-text#menu-institucional",
      "menu-item#quem-somos",
      "menu-item#politica-e-seguranca",
      "menu-item#trabalhe-conosco",
      "rich-text#menu-atendimento",
      "menu-item#fale-conosco",
      "menu-item#forma-de-entrega",
      "menu-item#pagamento",
      "menu-item#prazos-de-envio",
      "menu-item#troca-e-devolucao",
      "menu-item#duvidas-frequentes"
    ]
  }
}

