{
  "responsive-layout.desktop#banner": {
    "children": ["flex-layout.row#image-banner-institucional"]
  },
  "flex-layout.row#image-banner-institucional": {
    "title": "Banner principal",
    "props": {
      "fullWidth": true,
      "blockClass": ["container__banner-institucional"]
    },
    "children": [ "ImageComponent#institucional"]
  },

  "ImageComponent#institucional": {
    "props": {
      "className": "Imagem-Home",
      "exibirImagem": true,
      "imageUrl": "https://via.placeholder.com/1252x280",
      "imageUrlMobile": "https://via.placeholder.com/320x200",
      "alt": "Imagem",
      "imageStyle": {
        "marginTop":"10px",
        "maxWidth":"100%",
        "width": "100%"
      },
      "loadingType": "lazy"
    }
  }
}
