    "props": {
      "blockClass": "footer-payment-mobile"
    }
  },
  "rich-text#title-payment": {
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "text": "Formas de Pagamento",
      "blockClass": "title-payment"
    }
  },
  "image-list#payment-card-mobile": {
    "props": {
      "blockClass": "payment-img-card",
