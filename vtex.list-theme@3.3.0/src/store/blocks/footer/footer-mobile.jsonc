{
  "flex-layout.row#footer-mobile-1": {
    "children": [
      "flex-layout.col#footer-payment-mobile",
      "flex-layout.col#footer-safebuy-mobile",
      "flex-layout.col#footer-socialnetwork-mobile"
    ],
    "props": {
      "blockClass": ["container", "footer-mobile-1"]
    }
  },
  "flex-layout.row#footer-mobile-2": {
